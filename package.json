{
  "name": "inialum-lp",
  "type": "module",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "pages:preview": "astro build && wrangler pages dev ./dist",
    "pages:deploy": "if test \"$IS_PRODUCTION_DEPLOY\" = \"true\" ; then astro build && wrangler pages deploy ./dist --project-name inialum-lp; else astro build && wrangler pages deploy ./dist --project-name inialum-lp-staging --branch main; fi",
    "lint": "pnpm run lint:check && pnpm run format:check",
    "lint:check": "eslint --cache \"**/*.{js,jsx,ts,tsx,astro}\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx,astro}\" --ignore-path .gitignore .",
    "format": "pnpm run format:fix",
    "format:check": "prettier --cache --check \"**/*.{js,jsx,ts,tsx,json,md,astro}\"",
    "format:fix": "prettier --cache --write \"**/*.{js,jsx,ts,tsx,json,md,astro}\"",
    "storybook:dev": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^5.1.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.14.5",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240821.1",
    "@inialum/eslint-config": "^1.2.0",
    "@inialum/memories-css": "0.3.0",
    "@inialum/memories-react": "0.3.0",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss-nesting": "^13.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.33.5",
    "storybook": "^8.2.9",
    "wrangler": "^3.72.2"
  },
  "packageManager": "pnpm@9.1.3+sha256.7f63001edc077f1cff96cacba901f350796287a2800dfa83fe898f94183e4f5f"
}
