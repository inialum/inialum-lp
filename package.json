{
  "name": "inialum-lp",
  "type": "module",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "pages:preview": "astro build && wrangler pages dev ./dist",
    "pages:deploy": "if test \"$IS_PRODUCTION_DEPLOY\" = \"true\" ; then astro build && wrangler pages deploy ./dist --project-name inialum-lp; else astro build && wrangler pages deploy ./dist --project-name inialum-lp-staging --branch main; fi",
    "lint": "pnpm run lint:check && pnpm run format:check",
    "lint:check": "eslint --cache \"**/*.{js,jsx,ts,tsx,astro}\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx,astro}\" --ignore-path .gitignore .",
    "format": "pnpm run format:fix",
    "format:check": "prettier --cache --check \"**/*.{js,jsx,ts,tsx,json,md,astro}\"",
    "format:fix": "prettier --cache --write \"**/*.{js,jsx,ts,tsx,json,md,astro}\"",
    "storybook:dev": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/react": "^3.1.0",
    "@astrojs/tailwind": "^5.1.0",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "astro": "^4.5.8",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240320.1",
    "@inialum/eslint-config": "^1.1.1",
    "@storybook/addon-a11y": "^8.0.2",
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/blocks": "^8.0.2",
    "@storybook/react": "^8.0.2",
    "@storybook/react-vite": "^8.0.2",
    "@storybook/test": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.32.0",
    "eslint-plugin-storybook": "^0.8.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "sharp": "^0.33.2",
    "storybook": "^8.0.2",
    "wrangler": "^3.36.0"
  }
}
